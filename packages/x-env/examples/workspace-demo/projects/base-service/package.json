{
  "name": "base-service",
  "version": "1.0.0",
  "description": "Base service with database and Redis configuration using @dweb-cloud/safenv",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsdown src/index.ts --format esm --format cjs",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@dweb-cloud/safenv": "workspace:*",
    "dotenv": "^16.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsdown": "^0.2.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "safenv",
    "environment-variables",
    "configuration",
    "base-service"
  ],
  "author": "",
