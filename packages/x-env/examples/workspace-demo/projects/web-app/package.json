{
  "name": "web-app",
  "version": "1.0.0",
  "description": "Web application that depends on base-service and demonstrates @dweb-cloud/safenv autoDependencies",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsdown src/index.ts --format esm --format cjs",
    "dev": "node --experimental-strip-types src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@dweb-cloud/safenv": "workspace:*",
    "base-service": "workspace:*",
    "dotenv": "^16.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsdown": "^0.2.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "safenv",
    "environment-variables",
    "configuration",
    "web-app",
    "dependencies"
  ],
  "author": "",
  "license": "MIT"
}
